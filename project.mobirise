          "_name": "testimonials5",
          "_customHTML": "<section class=\"testimonials5 slider-with-bullets\" group=\"Testimonials\" plugins=\"amp-main, amp-carousel, amp-bind\">\n\n\t<mbr-parameters>\n\t\t<!-- Block parameters controls (Blue \"Gear\" panel) -->\n\t\t<input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"8\" step=\"1\" value=\"2\">\n\t\t<input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"8\" step=\"1\" value=\"4\">\n\n\t\t<input type=\"checkbox\" title=\"Show Name\" name=\"showname\" checked=\"\">\n\t\t<select title=\"Slides Count\" name=\"imagesAmount\">\n\t\t\t<option value=\"1\">1</option>\n\t\t\t<option value=\"2\" selected=\"\">2</option>\n\t\t\t<option value=\"3\">3</option>\n\t\t\t<option value=\"4\">4</option>\n\t\t\t<option value=\"5\">5</option>\n\t\t\t<option value=\"6\">6</option>\n\t\t\t<option value=\"7\">7</option>\n\t\t\t<option value=\"8\">8</option>\n\t\t</select>\n\t\t<input type=\"checkbox\" title=\"Autoplay\" name=\"autoplay\" checked=\"\" condition=\"imagesAmount > 1\">\n\t\t<input type=\"range\" inline=\"\" title=\"Delay\" name=\"delay\" min=\"1000\" max=\"5000\" step=\"1000\" value=\"3000\" condition=\"autoplay &amp;&amp; imagesAmount > 1\">\n\t\t<input type=\"color\" title=\"Text BG Color\" name=\"bgColor\" value=\"#ff4848\">\n\t\t<input type=\"checkbox\" title=\"Show Bullets\" name=\"showBullets\" checked=\"\">\n\t\t<input type=\"color\" title=\"Bullets Color\" name=\"bulletsColor\" value=\"#ff4848\" condition=\"showBullets\">\n\t\t<input type=\"color\" title=\"Active Bullet Color\" name=\"activeBullet\" value=\"#044d93\" condition=\"showBullets\">\n\n\t\t<fieldset type=\"background\" name=\"bg\">\n\t\t\t<input type=\"image\" title=\"Background Image\" value=\"../_images/background1.jpg\">\n\t\t\t<input type=\"color\" title=\"Background Color\" value=\"#f7f7f7\" selected=\"\">\n\t\t</fieldset>\n\n\t\t<input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" checked=\"\" condition=\"bg.type!='color'\">\n\t\t<input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"bg.type!='color' &amp;&amp; overlay\">\n\t\t<input type=\"range\" inline=\"\" title=\"Overlay Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"bg.type!='color' &amp;&amp; overlay\">\n\t\t<!-- End block parameters -->\n\t</mbr-parameters>\n\n\t<div class=\"mbr-overlay\" mbr-if=\"overlay &amp;&amp; bg.type !== 'color'\"></div>\n\t<div class=\"container\">\n\n\t\t<div class=\"slider-box\" data-autoplay=\"{{autoplay}}\" data-delay=\"{{delay}}\">\n\t\t\t<amp-carousel class=\"carousel\" height=\"9\" width=\"16\" layout=\"responsive\" type=\"slides\" id=\"carousel1\" loop=\"\" on=\"slideChange:AMP.setState({selectedSlide: event.index})\">\n\t\t\t\t<div class=\"slide slide-wrap\">\n\t\t\t\t\t<div class=\"item-card\">\n\n\n\n\t\t\t\t\t\t<div class=\"blockname\">\n\t\t\t\t\t\t\t<div class=\"img_wrapper\">\n\t\t\t\t\t\t\t\t<amp-img src=\"@PROJECT_PATH@/assets/images/renny-close-up-146x146.jpg\" alt=\"Mobirise\" width=\"9\" height=\"9\" layout=\"responsive\">\n\t\t\t\t\t\t\t\t\t<div placeholder=\"\">\n\t\t\t\t\t\t\t\t\t\t<div class=\"mobirise-spinner\">\n\t\t\t\t\t\t\t\t\t\t\t<em></em>\n\t\t\t\t\t\t\t\t\t\t\t<em></em>\n\t\t\t\t\t\t\t\t\t\t\t<em></em>\n\t\t\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t\t\t<img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/renny-close-up-146x146.jpg\" alt=\"Mobirise\" title=\"\">\n\t\t\t\t\t\t\t\t</amp-img>\n\t\t\t\t\t\t\t</div>\n\n\t\t\t\t\t\t\t<div class=\"name_wrap\">\n\n\t\t\t\t\t\t\t\t<h3 class=\"mbr-fonts-style name\" mbr-theme-style=\"display-3\" mbr-if=\"showname\" data-app-selector=\".name\">\n\t\t\t\t\t\t\t\t\t<b>Renny Castillo</b></h3>\n\t\t\t\t\t\t\t\t<h4 class=\"mbr-fonts-style status\" mbr-theme-style=\"display-5\" mbr-if=\"showname\" data-app-selector=\".status\">Client</h4>\n\n\t\t\t\t\t\t\t</div>\n\n\t\t\t\t\t\t</div>\n\n\t\t\t\t\t\t<div class=